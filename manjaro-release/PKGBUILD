pkgname=manjaro-release
pkgver=16.12
pkgrel=4
pkgdesc="Manjaro's release definition"
arch=("any")
url="http://manjarolinux.org/"
license=('GPL2')
depends=('lsb-release')
source=('lsb-release')
install="manjaro-release.install"
sha256sums=('cea457839bfbc4654c96a6f914c686fd61fe8e809536be23828a6f0a3cd33585')

package() {
    # Copy files
    mkdir -p ${pkgdir}/etc
    install -m644 ${srcdir}/lsb-release ${pkgdir}/etc/

}

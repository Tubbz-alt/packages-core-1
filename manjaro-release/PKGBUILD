pkgname=manjaro-release
pkgver=16.04
pkgrel=2
pkgdesc="Manjaro's release definition"
arch=("any")
url="http://manjarolinux.org/"
license=('GPL2')
depends=('lsb-release')
source=('lsb-release')
install="manjaro-release.install"
sha256sums=('383769713f73aefd73127f61ca6feb2b86f93b98da6a8fc6b467ea9cbe554006')

package() {
    # Copy files
    mkdir -p ${pkgdir}/etc
    install -m644 ${srcdir}/lsb-release ${pkgdir}/etc/

}

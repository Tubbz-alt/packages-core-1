pkgname=manjaro-release
pkgver=16.08
pkgrel=3
pkgdesc="Manjaro's release definition"
arch=("any")
url="http://manjarolinux.org/"
license=('GPL2')
depends=('lsb-release')
source=('lsb-release')
install="manjaro-release.install"
sha256sums=('109451256899be08a3a9d759bb3acf16ac6c84fd2ad566e35eeb7aeda2e6c2c7')

package() {
    # Copy files
    mkdir -p ${pkgdir}/etc
    install -m644 ${srcdir}/lsb-release ${pkgdir}/etc/

}

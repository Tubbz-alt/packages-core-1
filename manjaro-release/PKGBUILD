pkgname=manjaro-release
pkgver=18.03
pkgrel=1
pkgdesc="Manjaro ARM's release definition"
arch=("any")
url="http://manjaroarm.org/"
license=('GPL2')
depends=('lsb-release')
source=('lsb-release')
install="manjaro-release.install"
sha256sums=('09ef7e9c52e12dc7ca74547c11e9130dbc7042de1720d4c62bcb70dacde383db')

package() {
    # Copy files
    mkdir -p ${pkgdir}/etc
    install -m644 ${srcdir}/lsb-release ${pkgdir}/etc/

}

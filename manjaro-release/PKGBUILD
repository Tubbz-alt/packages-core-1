pkgname=manjaro-release
pkgver=18.04
pkgrel=1
pkgdesc="Manjaro ARM's release definition"
arch=("any")
url="http://manjaroarm.org/"
license=('GPL2')
depends=('lsb-release')
source=('lsb-release')
install="manjaro-release.install"
sha256sums=('640f77e79a01e2f6f0f3687857630a450e29d7fc97df037b02b7f329d9b93dc2')

package() {
    # Copy files
    mkdir -p ${pkgdir}/etc
    install -m644 ${srcdir}/lsb-release ${pkgdir}/etc/

}

pkgname=manjaro-release
pkgver=18.05
pkgrel=1
pkgdesc="Manjaro ARM's release definition"
arch=("any")
url="http://manjaroarm.org/"
license=('GPL2')
depends=('lsb-release')
source=('lsb-release')
install="manjaro-release.install"
sha256sums=('6ab84c3e9162a2dbf93ff557df6fcc50390bf2ab5e2bd880a9f5adb367ccd149')

package() {
    # Copy files
    mkdir -p ${pkgdir}/etc
    install -m644 ${srcdir}/lsb-release ${pkgdir}/etc/

}

pkgname=manjaro-release
pkgver=18.02
pkgrel=1
pkgdesc="Manjaro's release definition"
arch=("any")
url="http://manjarolinux.org/"
license=('GPL2')
depends=('lsb-release')
source=('lsb-release')
install="manjaro-release.install"
sha256sums=('fac2f930623cdcce47a3f3fd7b2b430fe02deebbbe4890ae368fc75a70368e64')

package() {
    # Copy files
    mkdir -p ${pkgdir}/etc
    install -m644 ${srcdir}/lsb-release ${pkgdir}/etc/

}
